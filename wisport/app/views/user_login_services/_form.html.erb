<%= simple_form_for(@user_login_service, html: {class: 'form-horizontal' }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :provider %>
    <%= f.input :uid %>
    <%= f.input :login %>
    <%= f.input :acces_token %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
  </div>
<% end %>
